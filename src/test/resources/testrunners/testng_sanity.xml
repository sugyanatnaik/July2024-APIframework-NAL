<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="API Regression Suite" verbose="3">


	<listeners>
		<listener
			class-name="com.qa.api.listeners.ExtentReportListener" />

	</listeners>


	<test name="GET User Test">

		<classes>
			<class name="com.qa.api.tests.GetUserTest" />
		</classes>
	</test>

	<test name="Req Res API Test">

		<classes>
			<class name="com.qa.reqres.api.tests.ReqResTest" />
		</classes>
	</test>

	<test name="Contacts API Test">

		<classes>
			<class name="com.qa.contacts.api.tests.ContactsAPITest" />
		</classes>
	</test>


	<test name="MOCK API Test">
		<classes>
			<class name="com.qa.api.mocking.tests.MockGetProductAPITest" />
		</classes>
	</test>






</suite>